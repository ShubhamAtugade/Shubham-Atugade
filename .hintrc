{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "scrollbar-width",
          "scrollbar-width: none"
        ]
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off",
        "list": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "meta-viewport": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "css-prefix-order": "off"
  }
}